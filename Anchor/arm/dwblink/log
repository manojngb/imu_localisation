mkdir -p build
arm-none-eabi-gcc -g -O2            -Tstm32_flash.ld -DUSE_STDPERIPH_DRIVER -mlittle-endian -mthumb -mthumb-interwork -mcpu=cortex-m4 -msoft-float -I. -I../common/stm32f4discovery/Libraries/CMSIS/ST/STM32F4xx/Include -I../common/stm32f4discovery/Libraries/CMSIS/Include -I../common/stm32f4discovery/Libraries/STM32F4xx_StdPeriph_Driver/inc -I../common/stm32f4discovery/Libraries/STM32_USB_OTG_Driver/inc -I../common/stm32f4discovery/Libraries/STM32_USB_Device_Library/Core/inc -I../common/stm32f4discovery/Libraries/STM32_USB_Device_Library/Class/cdc/inc -Ivcp/inc -Ileds/inc -Idw/decadriver/inc -Idw/application/inc main.c stm32f4xx_it.c system_stm32f4xx.c vcp/src/usb_bsp.c vcp/src/usbd_cdc_vcp.c vcp/src/usbd_desc.c vcp/src/usbd_usr.c leds/src/leds.c dw/decadriver/src/deca_device.c dw/decadriver/src/deca_params_init.c dw/decadriver/src/deca_range_tables.c dw/decadriver/src/deca_spi.c dw/application/src/deca_handler.c ../common/stm32f4discovery/Libraries/STM32F4xx_StdPeriph_Driver/src/stm32f4xx_exti.c ../common/stm32f4discovery/Libraries/STM32F4xx_StdPeriph_Driver/src/stm32f4xx_gpio.c ../common/stm32f4discovery/Libraries/STM32F4xx_StdPeriph_Driver/src/stm32f4xx_rcc.c ../common/stm32f4discovery/Libraries/STM32F4xx_StdPeriph_Driver/src/stm32f4xx_usart.c ../common/stm32f4discovery/Libraries/STM32F4xx_StdPeriph_Driver/src/stm32f4xx_rtc.c ../common/stm32f4discovery/Libraries/STM32F4xx_StdPeriph_Driver/src/misc.c ../common/stm32f4discovery/Libraries/STM32_USB_OTG_Driver/src/usb_core.c ../common/stm32f4discovery/Libraries/STM32_USB_OTG_Driver/src/usb_dcd.c ../common/stm32f4discovery/Libraries/STM32_USB_OTG_Driver/src/usb_dcd_int.c ../common/stm32f4discovery/Libraries/STM32_USB_Device_Library/Class/cdc/src/usbd_cdc_core.c ../common/stm32f4discovery/Libraries/STM32_USB_Device_Library/Core/src/usbd_core.c ../common/stm32f4discovery/Libraries/STM32_USB_Device_Library/Core/src/usbd_ioreq.c ../common/stm32f4discovery/Libraries/STM32_USB_Device_Library/Core/src/usbd_req.c ../common/stm32f4discovery/Libraries/STM32F4xx_StdPeriph_Driver/src/stm32f4xx_spi.c ../common/startup/startup_stm32f4xx.s -o build/stm32f4_dwspec.elf  
